{% extends '::layout_admin.html.twig' %}

{% block body %}
    <h1>Categorieprestas list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nomcategorie</th>
                <th>Ordreaffichage</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for categoriePrestum in categoriePrestas %}
            <tr>
                <td><a href="{{ path('admin_categorie-presta_show', { 'id': categoriePrestum.id }) }}">{{ categoriePrestum.id }}</a></td>
                <td>{{ categoriePrestum.nomCategorie }}</td>
                <td>{{ categoriePrestum.ordreAffichage }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('admin_categorie-presta_show', { 'id': categoriePrestum.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_categorie-presta_edit', { 'id': categoriePrestum.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_categorie-presta_new') }}">Create a new categoriePrestum</a>
        </li>
    </ul>
{% endblock %}
